    <div id="pinContainer2" class="text-white text-center">
        <section class="panel2 dark-brown">
            <div style="position: relative">

                     <img src="static/neon.gif" class="neon" alt="">
                     <img src="static/door-close.png" id="myimg" style="z-index: 0;" class="door pb-5"><br>
                <div id="target" data-aos="fade-in">
                    <img id="plane" width="100px" style="position: absolute;left: 0;top: 50px;z-index: 10" src="static/leaf.png">
                </div>
            </div>
        </section>
        <p id="paintSets" class="d-none"> <%=paintSets%> </p>
        <p id="paints" class="d-none"> <%=paints%> </p>
        <% paintSets = JSON.parse(paintSets) %>
        <% paints = JSON.parse(paints) %>
        <% var paintings = []; %>
        <% for(var i=0;i<paintSets.length;i++) { %>
        <%
                paintings = []
                var j = i+1;
                for(var k=0;k<paints.length;k++) {
                    if(paints[k].set === paintSets[i]._id) {
                        paintings.push(paints[k]);
                    }
                }
        %>
            <section class="panel2 room<%=i%> bg<%=i%>" style="background-color: <%=paintSets[i].color%>">
            <div class="container">
                <div class="room-name pt-5 d-none d-lg-block">
                    <div class="row justify-content-start bold-font text-white text-center">
                        <div class="col-md-2">
                            <h3>ROOM<%=j%></h3>
                            <hr class="bg-white w-100">
                            <h4><%=paintSets[i].name%></h4>
                        </div>
                    </div>
                </div>
                <img src="static/canvas.png" class="canvas pb-5">
                <div class="carousel-caption carousel-caption-<%=i%> text-white container">
                    <div class="row justify-content-center">
                        <div class="col-md-6 col-sm-12">
                            <h2 class="bold-font"><%=paintings[0].name%></h2>
                            <hr class="w-25 bg-white">
                            <p class="italic-font"><%=paintings[0].caption%></p>
                        </div>
                    </div>
                    </div>
                <div id="carouselExampleControls<%=i%>" class="carousel slide in-canvas" data-ride="carousel">
                    <div class="carousel-inner">
                        <ol class="carousel-indicators">
                            <% for(var p=0;p<paintings.length;p++) { %>
                                   <% if(p===0) { %>
                            <li data-target="#carouselExampleControls<%=i%>" data-slide-to=<%=p%> class="active"></li>
                                    <% } else { %>
                            <li data-target="#carouselExampleControls<%=i%>" data-slide-to=<%=p%> ></li>
                                     <% } %>
                            <% } %>
                        </ol>
                        <% for(var t=0;t<paintings.length;t++) { %>
                            <% if(t===0) { %>
                            <div class="carousel-item fitter active">
                                <img src="<%=paintings[t].image%>" class="d-block" alt="...">
                            </div>
                            <% } else { %>
                            <div class="carousel-item fitter">
                                    <img src="<%=paintings[t].image%>" class="d-block" alt="...">
                            </div>
                            <% } %>
                        <% } %>
                    </div>

                    <a class="carousel-control-prev d-none" href="#carouselExampleControls<%=i%>" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next d-none" href="#carouselExampleControls<%=i%>" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>
        </section>
        <% } %>
    </div>

    <section id="salam" class="green text-center" style="min-height: 830px;position: relative" >
        <div class="container" style="padding-top: 40px">
            <div class="row text-center pb-5" style="padding-top: 100px">
                <div class="col-md-8 col-sm 12 text-white" data-aos="fade-right" >
                    <h1 class="xbig-font">About</h1>
                    <br>
                    <div class="container">
                        <p class="md-font text-break pb-5">
                            <%=details.about%>
                        </p>
                    </div>

                </div>
                <br>
                <div class="col-md-4 col-sm-12" style="padding-top: 25px;padding-bottom: 150px" data-aos="fade-left">
                    <div class="card yellow pb-5">
                        <div class="card-block">
                            <div class="container">
                                <h3 class="card-title pt-3 text-center">
                                    Contact
                                </h3>
                                <form>
                                    <label for="instagram">Instagram</label>
                                    <div class="input-group input-group-lg">
                                        <div class="bg-light p-3 w-100 sm-font" style="border-radius: 2%">
                                            <a id="instagram" class="text-dark" target="_blank" href="https://www.instagram.com/<%=details.instagram%>">
                                                @<%=details.instagram%>
                                            </a>
                                        </div>
                                    </div>
                                    <br>
                                    <label for="email">Email </label>
                                    <div class="input-group input-group-lg">
                                        <div class="bg-light p-3 w-100 sm-font" style="border-radius: 2%">
                                            <a class="text-dark" target="_blank" href="mailto:<%=details.email%>"><%=details.email%></a>
                                        </div>
                                    </div>
                                    <br>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="dark-brown text-white text-center w-100" style="height: 150px;bottom: 0;position: absolute">
            <p class="pt-5">Made By <a target="_blank" href="https://www.instagram.com/zosoist">@Zosoist</a> <br> Copyright © 2019</p>
        </div>
    </section>

</div>




